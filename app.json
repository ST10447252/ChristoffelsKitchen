{
  "expo": {
    "name": "ChristoffelsKitchen",
    "slug": "ChristoffelsKitchen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/CKlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/CKlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#16423C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assets": [
      "./assets/fonts"
    ],
    "plugins": [
      "expo-font"
    ]
  }
}
